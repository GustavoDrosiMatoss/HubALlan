    Title = "Allan Hub | Arise Crossover",
    SubTitle = "",
    TabWidth = 140,
    Size = UDim2.fromOffset(450, 350),
    Acrylic = false,
    Theme = "Luffy",
    MinimizeKey = Enum.KeyCode.LeftControl
})

